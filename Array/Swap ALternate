#include <iostream>
using namespace std ;
// SWAP ALTERNATE 
void swapAlter(int arr[], int size){
    int i = 0 ;
    while(i < size-1){
        swap(arr[i],arr[i+1]);
        i=i+2;
    }
}
//PRINTING ELEMENTS 
void arrPrint(int a[], int n ){
    for(int i = 0 ; i < n ; i++){
        cout<<a[i]<<" ";
    }
    cout<<endl;
}
int main() {
    
  int arr[7]= {23,34,54,672,13,64,1};
  swapAlter(arr,7);
  arrPrint(arr,7);
    return 0;
}
