#include <iostream>
using namespace std;

int main() {
  int n;
  cin>>n;
//   appling while loops
  int product = 1; 
  int sum =  0 ;
  while(n!= 0){
      int digit = n%10;
      product = product * digit;
      sum = sum + digit;
      n = n/10;
  }
  
   cout<<"Substraction of product adn sum of digits of an integers = ";
   cout<<product - sum <<endl;
    return 0;
}
